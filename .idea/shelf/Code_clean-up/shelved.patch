tion animation) {\n            // Start Main Screen\n            Intent i = new Intent(SplashActivity.this, WelcomeActivity.class);\n            startActivity(i);\n            this.finish();\n    }\n\n    @Override\n    public void onAnimationRepeat(Animation animation) {\n        //under Implementation\n    }\n\n}
